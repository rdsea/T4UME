/*
Returns a Set of Element annotated with Uncertainty.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package UncertaintysAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isUncertainty()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Uncertainty.
*/ 
operation Element isUncertainty() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isUncertainty() : Boolean in Uncertainty.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Uncertainty");
}


//getters
/*
Returns a Set of cause of stereotype Uncertainty.
*/ 
operation Element getCauseOfUncertaintyAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Uncertainty"), "cause");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


