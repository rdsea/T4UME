//--get evidence Property Definition. It does not change then it is cached.
@cache
operation Stereotype getEvidencePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var evidenceProperty = allAttributes.selectOne(s|s.name = "evidence");
	return evidenceProperty;
}

