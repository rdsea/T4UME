/*
Returns a Set of Element annotated with Measurement.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package MeasurementsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isMeasurement()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Measurement.
*/ 
operation Element isMeasurement() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMeasurement() : Boolean in Measurement.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Measurement");
}


//getters
/*
Returns a Set of evidence of stereotype Measurement.
*/ 
operation Element getEvidenceOfMeasurementAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Measurement"), "evidence");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


