/*
Returns a Set of Element annotated with BeliefStatement.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package BeliefStatementsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isBeliefStatement()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with BeliefStatement.
*/ 
operation Element isBeliefStatement() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isBeliefStatement() : Boolean in BeliefStatement.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "BeliefStatement");
}


//getters
/*
Returns a Set of uncertainty of stereotype BeliefStatement.
*/ 
operation Element getUncertaintyOfBeliefStatementAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("BeliefStatement"), "uncertainty");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


