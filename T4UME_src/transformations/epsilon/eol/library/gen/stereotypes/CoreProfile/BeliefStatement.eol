//--get uncertainty Property Definition. It does not change then it is cached.
@cache
operation Stereotype getUncertaintyPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var uncertaintyProperty = allAttributes.selectOne(s|s.name = "uncertainty");
	return uncertaintyProperty;
}

