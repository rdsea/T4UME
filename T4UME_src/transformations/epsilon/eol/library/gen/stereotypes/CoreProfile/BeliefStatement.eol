//setters
// -- Setter for Property BeliefStatement::uncertainty	
// org.eclipse.uml2.uml.internal.impl.ClassImpl@2b0fc42c (name: UncertaintyClass, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false) (isActive: false)
operation Element setUncertaintyProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_coreProfile = Profile.all.selectOne(e|e.name = "CoreProfile");
		
		var stereotype = current_coreProfile.getBeliefStatementStereotype();
		var property = current_coreProfile.getUncertaintyProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
