/*
Returns a Set of Element annotated with Locality.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package LocalitysAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isLocality()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Locality.
*/ 
operation Element isLocality() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isLocality() : Boolean in Locality.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Locality");
}


//getters
/*
Returns a Set of location of stereotype Locality.
*/ 
operation Element getLocationOfLocalityAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Locality"), "location");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


