}


/*
Check if a Set of location of stereotype Locality is empty.
*/ 
operation Element isLocationOfLocalityEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Locality"), "location");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
