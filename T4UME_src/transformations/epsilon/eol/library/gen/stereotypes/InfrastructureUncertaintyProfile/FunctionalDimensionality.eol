/*
Returns a Set of Element annotated with FunctionalDimensionality.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package FunctionalDimensionalitysAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isFunctionalDimensionality()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with FunctionalDimensionality.
*/ 
operation Element isFunctionalDimensionality() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isFunctionalDimensionality() : Boolean in FunctionalDimensionality.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "FunctionalDimensionality");
}


//getters
/*
Returns a Set of dimensionalty of stereotype FunctionalDimensionality.
*/ 
operation Element getDimensionaltyOfFunctionalDimensionalityAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("FunctionalDimensionality"), "dimensionalty");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


