//getters of Property Definitions
//--get dimensionalty Property Definition. It does not change then it is cached.
@cache
operation Stereotype getDimensionaltyPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var dimensionaltyProperty = allAttributes.selectOne(s|s.name = "dimensionalty");
	return dimensionaltyProperty;
}

//--get functional_attributes Property Definition. It does not change then it is cached.
@cache
operation Stereotype getFunctional_attributesPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var functional_attributesProperty = allAttributes.selectOne(s|s.name = "functional_attributes");
	return functional_attributesProperty;
}

//--get configuration Property Definition. It does not change then it is cached.
