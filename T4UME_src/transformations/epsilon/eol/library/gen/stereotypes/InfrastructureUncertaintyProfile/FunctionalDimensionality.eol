/*
Check if a Set of dimensionalty of stereotype FunctionalDimensionality is empty.
*/ 
operation Element isDimensionaltyOfFunctionalDimensionalityEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("FunctionalDimensionality"), "dimensionalty");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
