import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with NonFunctionalDimensionality.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package NonFunctionalDimensionalitysAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isNonFunctionalDimensionality()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with NonFunctionalDimensionality.
*/ 
operation Element isNonFunctionalDimensionality() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isNonFunctionalDimensionality() : Boolean in NonFunctionalDimensionality.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "NonFunctionalDimensionality");
}


//getters
/*
Returns a Set of description of stereotype NonFunctionalDimensionality.
*/ 
operation Element getDescriptionOfNonFunctionalDimensionalityAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("NonFunctionalDimensionality"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


