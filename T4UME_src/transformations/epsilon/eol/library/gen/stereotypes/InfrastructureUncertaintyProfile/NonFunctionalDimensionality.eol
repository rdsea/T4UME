//getters of Property Definitions
//--get description Property Definition. It does not change then it is cached.
@cache
operation Stereotype getDescriptionPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var descriptionProperty = allAttributes.selectOne(s|s.name = "description");
	return descriptionProperty;
}

//--get non_functional_attributes Property Definition. It does not change then it is cached.
@cache
operation Stereotype getNon_functional_attributesPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var non_functional_attributesProperty = allAttributes.selectOne(s|s.name = "non_functional_attributes");
	return non_functional_attributesProperty;
}

//--get configuration Property Definition. It does not change then it is cached.
@cache
operation Stereotype getConfigurationPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var configurationProperty = allAttributes.selectOne(s|s.name = "configuration");
	return configurationProperty;
}


/*
Check if a Set of description of stereotype NonFunctionalDimensionality is empty.
*/ 
operation Element isDescriptionOfNonFunctionalDimensionalityEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("NonFunctionalDimensionality"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
