/*
Returns a Set of Element annotated with Metric.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package MetricsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isMetric()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Metric.
*/ 
operation Element isMetric() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isMetric() : Boolean in Metric.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Metric");
}


//getters
/*
Returns a Set of description of stereotype Metric.
*/ 
operation Element getDescriptionOfMetricAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Metric"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


