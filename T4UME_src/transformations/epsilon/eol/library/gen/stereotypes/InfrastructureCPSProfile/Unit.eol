/*
Returns a Set of Element annotated with Unit.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package UnitsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isUnit()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Unit.
*/ 
operation Element isUnit() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isUnit() : Boolean in Unit.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Unit");
}


//getters
/*
Returns a Set of id of stereotype Unit.
*/ 
operation Element getIdOfUnitAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Unit"), "id");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


