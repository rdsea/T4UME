/*
Returns a Set of Element annotated with TestConfiguration.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package TestConfigurationsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isTestConfiguration()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with TestConfiguration.
*/ 
operation Element isTestConfiguration() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTestConfiguration() : Boolean in TestConfiguration.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "TestConfiguration");
}


//getters
/*
Returns a Set of description of stereotype TestConfiguration.
*/ 
operation Element getDescriptionOfTestConfigurationAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("TestConfiguration"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


