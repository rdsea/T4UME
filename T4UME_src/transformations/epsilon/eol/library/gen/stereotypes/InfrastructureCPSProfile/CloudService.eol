/*
Returns a Set of cloudProvider of stereotype CloudService.
*/ 
operation Element getCloudProviderOfCloudServiceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("CloudService"), "cloudProvider");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


