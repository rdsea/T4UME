//setters
// -- Setter for Property CloudService::cloudProvider	
// org.eclipse.uml2.uml.internal.impl.StereotypeImpl@37e6dd67 (name: CloudProvider, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false) (isActive: false)
operation Element setCloudProviderProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_infrastructureCPSProfile = Profile.all.selectOne(e|e.name = "InfrastructureCPSProfile");
		
		var stereotype = current_infrastructureCPSProfile.getCloudServiceStereotype();
		var property = current_infrastructureCPSProfile.getCloudProviderProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
