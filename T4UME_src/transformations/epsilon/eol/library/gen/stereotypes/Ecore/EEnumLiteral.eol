//getters of Property Definitions
//--get enumLiteralName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getEnumLiteralNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var enumLiteralNameProperty = allAttributes.selectOne(s|s.name = "enumLiteralName");
	return enumLiteralNameProperty;
}

//--get literal Property Definition. It does not change then it is cached.
@cache
operation Stereotype getLiteralPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var literalProperty = allAttributes.selectOne(s|s.name = "literal");
	return literalProperty;
}

