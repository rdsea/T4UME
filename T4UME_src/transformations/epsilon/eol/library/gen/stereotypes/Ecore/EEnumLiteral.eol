/*
Returns a Set of Element annotated with EEnumLiteral.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EEnumLiteralsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEEnumLiteral()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EEnumLiteral.
*/ 
operation Element isEEnumLiteral() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEEnumLiteral() : Boolean in EEnumLiteral.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EEnumLiteral");
}


//getters
/*
Returns a Set of enumLiteralName of stereotype EEnumLiteral.
*/ 
operation Element getEnumLiteralNameOfEEnumLiteralAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EEnumLiteral"), "enumLiteralName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


