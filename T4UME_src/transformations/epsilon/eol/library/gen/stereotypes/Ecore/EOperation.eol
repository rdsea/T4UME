import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with EOperation.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EOperationsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEOperation()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EOperation.
*/ 
operation Element isEOperation() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEOperation() : Boolean in EOperation.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EOperation");
}


//getters
/*
Returns a Set of operationName of stereotype EOperation.
*/ 
operation Element getOperationNameOfEOperationAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EOperation"), "operationName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


