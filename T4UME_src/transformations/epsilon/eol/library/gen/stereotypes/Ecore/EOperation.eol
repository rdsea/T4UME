//getters of Property Definitions
//--get operationName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOperationNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var operationNameProperty = allAttributes.selectOne(s|s.name = "operationName");
	return operationNameProperty;
}

//--get annotations Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAnnotationsPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var annotationsProperty = allAttributes.selectOne(s|s.name = "annotations");
	return annotationsProperty;
}

//--get visibility Property Definition. It does not change then it is cached.
@cache
operation Stereotype getVisibilityPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var visibilityProperty = allAttributes.selectOne(s|s.name = "visibility");
	return visibilityProperty;
}


/*
Check if a Set of operationName of stereotype EOperation is empty.
*/ 
operation Element isOperationNameOfEOperationEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("EOperation"), "operationName");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
