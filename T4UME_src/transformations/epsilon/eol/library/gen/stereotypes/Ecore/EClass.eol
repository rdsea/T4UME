//getters of Property Definitions
//--get className Property Definition. It does not change then it is cached.
@cache
operation Stereotype getClassNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var classNameProperty = allAttributes.selectOne(s|s.name = "className");
	return classNameProperty;
}

