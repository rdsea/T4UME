/*
Returns a Set of Element annotated with EClass.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EClasssAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEClass()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EClass.
*/ 
operation Element isEClass() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEClass() : Boolean in EClass.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EClass");
}


//getters
/*
Returns a Set of className of stereotype EClass.
*/ 
operation Element getClassNameOfEClassAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EClass"), "className");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


