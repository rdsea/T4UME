/*
Returns a Set of Element annotated with EPackage.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EPackagesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEPackage()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EPackage.
*/ 
operation Element isEPackage() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEPackage() : Boolean in EPackage.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EPackage");
}


//getters
/*
Returns a Set of packageName of stereotype EPackage.
*/ 
operation Element getPackageNameOfEPackageAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EPackage"), "packageName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


