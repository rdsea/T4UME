//getters of Property Definitions
//--get packageName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPackageNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var packageNameProperty = allAttributes.selectOne(s|s.name = "packageName");
	return packageNameProperty;
}

