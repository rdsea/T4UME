//setters
// -- Setter for Property EStructuralFeature::isTransient	
// org.eclipse.uml2.uml.internal.impl.PrimitiveTypeImpl@2164343e (name: EBoolean, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setIsTransientProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_ecore = Profile.all.selectOne(e|e.name = "Ecore");
		
		var stereotype = current_ecore.getEStructuralFeatureStereotype();
		var property = current_ecore.getIsTransientProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
