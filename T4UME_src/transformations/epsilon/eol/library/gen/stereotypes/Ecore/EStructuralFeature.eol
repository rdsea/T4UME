/*
Returns a Set of Element annotated with EStructuralFeature.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EStructuralFeaturesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEStructuralFeature()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EStructuralFeature.
*/ 
operation Element isEStructuralFeature() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEStructuralFeature() : Boolean in EStructuralFeature.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EStructuralFeature");
}


//getters
/*
Returns a Set of isTransient of stereotype EStructuralFeature.
*/ 
operation Element getIsTransientOfEStructuralFeatureAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EStructuralFeature"), "isTransient");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


