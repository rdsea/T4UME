//--get isTransient Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsTransientPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isTransientProperty = allAttributes.selectOne(s|s.name = "isTransient");
	return isTransientProperty;
}

