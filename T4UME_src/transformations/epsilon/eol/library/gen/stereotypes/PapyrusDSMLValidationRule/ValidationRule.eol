// -- Setter for Property ValidationRule::severity	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@3117a4a6 (name: Severity, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setSeverityProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_papyrusDSMLValidationRule = Profile.all.selectOne(e|e.name = "PapyrusDSMLValidationRule");
		
		var stereotype = current_papyrusDSMLValidationRule.getValidationRuleStereotype();
		var property = current_papyrusDSMLValidationRule.getSeverityProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
