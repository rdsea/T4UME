/*
Returns a Set of severity of stereotype ValidationRule.
*/ 
operation Element getSeverityOfValidationRuleAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("ValidationRule"), "severity");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


