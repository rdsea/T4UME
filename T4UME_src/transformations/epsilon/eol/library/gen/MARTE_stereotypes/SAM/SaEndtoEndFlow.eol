/*
Check if a Set of isSched of stereotype SaEndtoEndFlow is empty.
*/ 
operation Element isIsSchedOfSaEndtoEndFlowEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SaEndtoEndFlow"), "isSched");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
