//getters of Property Definitions
//--get isSched Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsSchedPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isSchedProperty = allAttributes.selectOne(s|s.name = "isSched");
	return isSchedProperty;
}

