/*
Returns a Set of Element annotated with SaEndtoEndFlow.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SaEndtoEndFlowsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSaEndtoEndFlow()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SaEndtoEndFlow.
*/ 
operation Element isSaEndtoEndFlow() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSaEndtoEndFlow() : Boolean in SaEndtoEndFlow.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SaEndtoEndFlow");
}


//getters
/*
Returns a Set of isSched of stereotype SaEndtoEndFlow.
*/ 
operation Element getIsSchedOfSaEndtoEndFlowAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SaEndtoEndFlow"), "isSched");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


