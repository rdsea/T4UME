/*
Returns a Set of isSched of stereotype SaAnalysisContext.
*/ 
operation Element getIsSchedOfSaAnalysisContextAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SaAnalysisContext"), "isSched");
	for(value in values){
