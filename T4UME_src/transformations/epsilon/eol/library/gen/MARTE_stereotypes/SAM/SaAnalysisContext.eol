/*
Returns a Set of Element annotated with SaAnalysisContext.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SaAnalysisContextsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSaAnalysisContext()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SaAnalysisContext.
*/ 
operation Element isSaAnalysisContext() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSaAnalysisContext() : Boolean in SaAnalysisContext.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SaAnalysisContext");
}


