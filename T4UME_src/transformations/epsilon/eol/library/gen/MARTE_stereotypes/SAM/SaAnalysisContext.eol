/*
Check if a Set of isSched of stereotype SaAnalysisContext is empty.
*/ 
operation Element isIsSchedOfSaAnalysisContextEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SaAnalysisContext"), "isSched");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
