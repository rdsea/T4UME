/*
Check if a Set of suspentions of stereotype SaSchedObs is empty.
*/ 
operation Element isSuspentionsOfSaSchedObsEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SaSchedObs"), "suspentions");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
