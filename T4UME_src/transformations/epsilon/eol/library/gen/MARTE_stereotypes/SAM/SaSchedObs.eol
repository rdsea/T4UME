/*
Returns a Set of Element annotated with SaSchedObs.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SaSchedObssAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSaSchedObs()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SaSchedObs.
*/ 
operation Element isSaSchedObs() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSaSchedObs() : Boolean in SaSchedObs.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SaSchedObs");
}


//getters
/*
Returns a Set of suspentions of stereotype SaSchedObs.
*/ 
operation Element getSuspentionsOfSaSchedObsAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SaSchedObs"), "suspentions");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


