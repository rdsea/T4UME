//--get suspentions Property Definition. It does not change then it is cached.
@cache
operation Stereotype getSuspentionsPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var suspentionsProperty = allAttributes.selectOne(s|s.name = "suspentions");
	return suspentionsProperty;
}

