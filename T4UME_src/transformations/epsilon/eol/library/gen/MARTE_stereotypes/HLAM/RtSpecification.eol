//--get utility Property Definition. It does not change then it is cached.
@cache
operation Stereotype getUtilityPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var utilityProperty = allAttributes.selectOne(s|s.name = "utility");
	return utilityProperty;
}

