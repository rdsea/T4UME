//setters
// -- Setter for Property RtSpecification::utility	
// org.eclipse.uml2.uml.internal.impl.DataTypeImpl@1620161e (name: UtilityType, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setUtilityProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hLAM = Profile.all.selectOne(e|e.name = "HLAM");
		
		var stereotype = current_hLAM.getRtSpecificationStereotype();
		var property = current_hLAM.getUtilityProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
