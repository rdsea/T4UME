/*
Returns a Set of Element annotated with RtSpecification.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package RtSpecificationsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isRtSpecification()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with RtSpecification.
*/ 
operation Element isRtSpecification() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isRtSpecification() : Boolean in RtSpecification.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "RtSpecification");
}


//getters
/*
Returns a Set of utility of stereotype RtSpecification.
*/ 
operation Element getUtilityOfRtSpecificationAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("RtSpecification"), "utility");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


