//getters of Property Definitions
//--get isAtomic Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsAtomicPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isAtomicProperty = allAttributes.selectOne(s|s.name = "isAtomic");
	return isAtomicProperty;
}

//--get synchKind Property Definition. It does not change then it is cached.
@cache
operation Stereotype getSynchKindPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var synchKindProperty = allAttributes.selectOne(s|s.name = "synchKind");
	return synchKindProperty;
}

//--get msgSize Property Definition. It does not change then it is cached.
@cache
operation Stereotype getMsgSizePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var msgSizeProperty = allAttributes.selectOne(s|s.name = "msgSize");
	return msgSizeProperty;
}


/*
Check if a Set of isAtomic of stereotype RtAction is empty.
*/ 
operation Element isIsAtomicOfRtActionEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("RtAction"), "isAtomic");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
