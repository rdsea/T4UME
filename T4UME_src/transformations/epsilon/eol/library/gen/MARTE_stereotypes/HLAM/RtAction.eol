import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with RtAction.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package RtActionsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isRtAction()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with RtAction.
*/ 
operation Element isRtAction() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isRtAction() : Boolean in RtAction.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "RtAction");
}


//getters
/*
Returns a Set of isAtomic of stereotype RtAction.
*/ 
operation Element getIsAtomicOfRtActionAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("RtAction"), "isAtomic");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


