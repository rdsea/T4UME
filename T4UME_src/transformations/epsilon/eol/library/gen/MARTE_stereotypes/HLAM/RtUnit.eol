/*
Returns a Set of isDynamic of stereotype RtUnit.
*/ 
operation Element getIsDynamicOfRtUnitAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("RtUnit"), "isDynamic");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


