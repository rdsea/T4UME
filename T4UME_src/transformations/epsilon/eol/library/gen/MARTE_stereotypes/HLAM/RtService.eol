//getters of Property Definitions
//--get concPolicy Property Definition. It does not change then it is cached.
@cache
operation Stereotype getConcPolicyPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var concPolicyProperty = allAttributes.selectOne(s|s.name = "concPolicy");
	return concPolicyProperty;
}

