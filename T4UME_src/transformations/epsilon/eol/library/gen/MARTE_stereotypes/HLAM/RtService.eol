/*
Returns a Set of Element annotated with RtService.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package RtServicesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isRtService()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with RtService.
*/ 
operation Element isRtService() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isRtService() : Boolean in RtService.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "RtService");
}


//getters
/*
Returns a Set of concPolicy of stereotype RtService.
*/ 
operation Element getConcPolicyOfRtServiceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("RtService"), "concPolicy");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


