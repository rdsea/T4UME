//setters
// -- Setter for Property RtService::concPolicy	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@2d3c35e0 (name: ConcurrencyKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setConcPolicyProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hLAM = Profile.all.selectOne(e|e.name = "HLAM");
		
		var stereotype = current_hLAM.getRtServiceStereotype();
		var property = current_hLAM.getConcPolicyProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
