/*
Returns a Set of Element annotated with ClockType.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package ClockTypesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isClockType()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with ClockType.
*/ 
operation Element isClockType() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isClockType() : Boolean in ClockType.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "ClockType");
}


//getters
/*
Returns a Set of nature of stereotype ClockType.
*/ 
operation Element getNatureOfClockTypeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("ClockType"), "nature");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


