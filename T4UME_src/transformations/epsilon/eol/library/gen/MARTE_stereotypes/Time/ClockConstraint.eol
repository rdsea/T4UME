//getters of Property Definitions
//--get isCoincidenceBased Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsCoincidenceBasedPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isCoincidenceBasedProperty = allAttributes.selectOne(s|s.name = "isCoincidenceBased");
	return isCoincidenceBasedProperty;
}

