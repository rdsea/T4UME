/*
Returns a Set of Element annotated with ClockConstraint.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package ClockConstraintsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isClockConstraint()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with ClockConstraint.
*/ 
operation Element isClockConstraint() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isClockConstraint() : Boolean in ClockConstraint.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "ClockConstraint");
}


