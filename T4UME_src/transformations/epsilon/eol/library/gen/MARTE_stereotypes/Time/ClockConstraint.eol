/*
Returns a Set of isCoincidenceBased of stereotype ClockConstraint.
*/ 
operation Element getIsCoincidenceBasedOfClockConstraintAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("ClockConstraint"), "isCoincidenceBased");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


