/*
Check if a Set of isCoincidenceBased of stereotype ClockConstraint is empty.
*/ 
operation Element isIsCoincidenceBasedOfClockConstraintEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("ClockConstraint"), "isCoincidenceBased");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
