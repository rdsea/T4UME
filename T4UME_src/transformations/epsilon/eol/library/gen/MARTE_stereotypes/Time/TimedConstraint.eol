//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with TimedConstraint.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package TimedConstraintsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isTimedConstraint()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with TimedConstraint.
*/ 
operation Element isTimedConstraint() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTimedConstraint() : Boolean in TimedConstraint.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "TimedConstraint");
}


//getters
/*
Returns a Set of interpretation of stereotype TimedConstraint.
*/ 
operation Element getInterpretationOfTimedConstraintAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("TimedConstraint"), "interpretation");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


