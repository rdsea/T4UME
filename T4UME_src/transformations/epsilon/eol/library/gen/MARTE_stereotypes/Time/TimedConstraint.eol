//getters of Property Definitions
//--get interpretation Property Definition. It does not change then it is cached.
@cache
operation Stereotype getInterpretationPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var interpretationProperty = allAttributes.selectOne(s|s.name = "interpretation");
	return interpretationProperty;
}

//--get kind Property Definition. It does not change then it is cached.
@cache
operation Stereotype getKindPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var kindProperty = allAttributes.selectOne(s|s.name = "kind");
	return kindProperty;
}

//--get mode Property Definition. It does not change then it is cached.
@cache
operation Stereotype getModePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var modeProperty = allAttributes.selectOne(s|s.name = "mode");
	return modeProperty;
}

//--get on Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOnPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var onProperty = allAttributes.selectOne(s|s.name = "on");
	return onProperty;
}


/*
Check if a Set of interpretation of stereotype TimedConstraint is empty.
*/ 
operation Element isInterpretationOfTimedConstraintEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("TimedConstraint"), "interpretation");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
