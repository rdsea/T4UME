/*
Returns a Set of Element annotated with TimedProcessing.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package TimedProcessingsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isTimedProcessing()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with TimedProcessing.
*/ 
operation Element isTimedProcessing() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTimedProcessing() : Boolean in TimedProcessing.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "TimedProcessing");
}


//getters
/*
Returns a Set of duration of stereotype TimedProcessing.
*/ 
operation Element getDurationOfTimedProcessingAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("TimedProcessing"), "duration");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


