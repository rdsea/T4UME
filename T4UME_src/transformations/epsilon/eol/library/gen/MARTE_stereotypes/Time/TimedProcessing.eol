/*
Check if a Set of duration of stereotype TimedProcessing is empty.
*/ 
operation Element isDurationOfTimedProcessingEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("TimedProcessing"), "duration");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
