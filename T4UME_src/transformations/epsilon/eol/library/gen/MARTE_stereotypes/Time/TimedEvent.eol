//getters of Property Definitions
//--get repetition Property Definition. It does not change then it is cached.
@cache
operation Stereotype getRepetitionPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var repetitionProperty = allAttributes.selectOne(s|s.name = "repetition");
	return repetitionProperty;
}

//--get every Property Definition. It does not change then it is cached.
@cache
operation Stereotype getEveryPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var everyProperty = allAttributes.selectOne(s|s.name = "every");
	return everyProperty;
}

//--get on Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOnPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var onProperty = allAttributes.selectOne(s|s.name = "on");
	return onProperty;
}


/*
Check if a Set of repetition of stereotype TimedEvent is empty.
*/ 
operation Element isRepetitionOfTimedEventEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("TimedEvent"), "repetition");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
