import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with TimedEvent.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package TimedEventsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isTimedEvent()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with TimedEvent.
*/ 
operation Element isTimedEvent() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTimedEvent() : Boolean in TimedEvent.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "TimedEvent");
}


//getters
/*
Returns a Set of repetition of stereotype TimedEvent.
*/ 
operation Element getRepetitionOfTimedEventAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("TimedEvent"), "repetition");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


