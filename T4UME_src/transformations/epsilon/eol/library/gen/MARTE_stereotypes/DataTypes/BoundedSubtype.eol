//setters
// -- Setter for Property BoundedSubtype::baseType	
// org.eclipse.uml2.uml.internal.impl.ClassImpl@7597041c (name: DataType, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false) (isActive: false)
operation Element setBaseTypeProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_dataTypes = Profile.all.selectOne(e|e.name = "DataTypes");
		
		var stereotype = current_dataTypes.getBoundedSubtypeStereotype();
		var property = current_dataTypes.getBaseTypeProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
