//--get baseType Property Definition. It does not change then it is cached.
@cache
operation Stereotype getBaseTypePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var baseTypeProperty = allAttributes.selectOne(s|s.name = "baseType");
	return baseTypeProperty;
}

