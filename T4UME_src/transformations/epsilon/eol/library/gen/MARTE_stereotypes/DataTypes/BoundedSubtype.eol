/*
Returns a Set of Element annotated with BoundedSubtype.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package BoundedSubtypesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isBoundedSubtype()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with BoundedSubtype.
*/ 
operation Element isBoundedSubtype() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isBoundedSubtype() : Boolean in BoundedSubtype.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "BoundedSubtype");
}


//getters
/*
Returns a Set of baseType of stereotype BoundedSubtype.
*/ 
operation Element getBaseTypeOfBoundedSubtypeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("BoundedSubtype"), "baseType");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


