//setters
// -- Setter for Property EClassifier::instanceClassName	
// org.eclipse.uml2.uml.internal.impl.PrimitiveTypeImpl@409bcd21 (name: EString, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setInstanceClassNameProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_ecore = Profile.all.selectOne(e|e.name = "Ecore");
		
		var stereotype = current_ecore.getEClassifierStereotype();
		var property = current_ecore.getInstanceClassNameProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
