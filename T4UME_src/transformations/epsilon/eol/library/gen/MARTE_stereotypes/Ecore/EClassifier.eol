//getters of Property Definitions
//--get instanceClassName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getInstanceClassNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var instanceClassNameProperty = allAttributes.selectOne(s|s.name = "instanceClassName");
	return instanceClassNameProperty;
}

//--get xmlName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getXmlNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var xmlNameProperty = allAttributes.selectOne(s|s.name = "xmlName");
	return xmlNameProperty;
}

//--get annotations Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAnnotationsPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var annotationsProperty = allAttributes.selectOne(s|s.name = "annotations");
	return annotationsProperty;
}


/*
Check if a Set of instanceClassName of stereotype EClassifier is empty.
*/ 
operation Element isInstanceClassNameOfEClassifierEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("EClassifier"), "instanceClassName");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
