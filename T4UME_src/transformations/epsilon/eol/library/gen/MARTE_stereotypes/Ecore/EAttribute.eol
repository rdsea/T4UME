/*
Returns a Set of Element annotated with EAttribute.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EAttributesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEAttribute()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EAttribute.
*/ 
operation Element isEAttribute() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEAttribute() : Boolean in EAttribute.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EAttribute");
}


//getters
/*
Returns a Set of attributeName of stereotype EAttribute.
*/ 
operation Element getAttributeNameOfEAttributeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EAttribute"), "attributeName");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


