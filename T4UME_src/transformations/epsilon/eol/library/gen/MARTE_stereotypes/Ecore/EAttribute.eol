//--get attributeName Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAttributeNamePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var attributeNameProperty = allAttributes.selectOne(s|s.name = "attributeName");
	return attributeNameProperty;
}

