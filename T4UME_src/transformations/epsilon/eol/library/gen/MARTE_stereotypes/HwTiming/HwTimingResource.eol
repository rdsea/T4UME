/*
Check if a Set of description of stereotype HwTimingResource is empty.
*/ 
operation Element isDescriptionOfHwTimingResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("HwTimingResource"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
