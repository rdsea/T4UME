/*
Returns a Set of Element annotated with HwTimer.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwTimersAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwTimer()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwTimer.
*/ 
operation Element isHwTimer() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwTimer() : Boolean in HwTimer.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwTimer");
}


//getters
/*
Returns a Set of nbCounters of stereotype HwTimer.
*/ 
operation Element getNbCountersOfHwTimerAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwTimer"), "nbCounters");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


