/*
Check if a Set of nbCounters of stereotype HwTimer is empty.
*/ 
operation Element isNbCountersOfHwTimerEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("HwTimer"), "nbCounters");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
