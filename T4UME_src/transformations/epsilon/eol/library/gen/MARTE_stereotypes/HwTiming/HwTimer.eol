//setters
// -- Setter for Property HwTimer::nbCounters	
// org.eclipse.uml2.uml.internal.impl.DataTypeImpl@7e522957 (name: NFP_Natural, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setNbCountersProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hwTiming = Profile.all.selectOne(e|e.name = "HwTiming");
		
		var stereotype = current_hwTiming.getHwTimerStereotype();
		var property = current_hwTiming.getNbCountersProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
