/*
Returns a Set of Element annotated with HwResource.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwResourcesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwResource()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwResource.
*/ 
operation Element isHwResource() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwResource() : Boolean in HwResource.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwResource");
}


//getters
/*
Returns a Set of description of stereotype HwResource.
*/ 
operation Element getDescriptionOfHwResourceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwResource"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


