/*
Check if a Set of description of stereotype HwResource is empty.
*/ 
operation Element isDescriptionOfHwResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("HwResource"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
