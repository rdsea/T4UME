// -- Setter for Property HwComponent::kind	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@2e2c5dd2 (name: ComponentKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setKindProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hwLayout = Profile.all.selectOne(e|e.name = "HwLayout");
		
		var stereotype = current_hwLayout.getHwComponentStereotype();
		var property = current_hwLayout.getKindProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
