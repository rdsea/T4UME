/*
Returns a Set of kind of stereotype HwComponent.
*/ 
operation Element getKindOfHwComponentAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwComponent"), "kind");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


