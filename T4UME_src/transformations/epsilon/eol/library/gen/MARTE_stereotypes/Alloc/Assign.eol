/*
Returns a Set of Element annotated with Assign.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package AssignsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isAssign()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Assign.
*/ 
operation Element isAssign() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isAssign() : Boolean in Assign.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Assign");
}


//getters
/*
Returns a Set of kind of stereotype Assign.
*/ 
operation Element getKindOfAssignAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Assign"), "kind");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


