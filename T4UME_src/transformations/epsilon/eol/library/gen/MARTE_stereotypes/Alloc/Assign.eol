}


/*
Check if a Set of kind of stereotype Assign is empty.
*/ 
operation Element isKindOfAssignEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Assign"), "kind");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
