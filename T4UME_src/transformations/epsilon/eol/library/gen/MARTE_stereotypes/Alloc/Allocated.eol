import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with Allocated.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package AllocatedsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isAllocated()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Allocated.
*/ 
operation Element isAllocated() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isAllocated() : Boolean in Allocated.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Allocated");
}


//getters
/*
Returns a Set of allocatedTo of stereotype Allocated.
*/ 
operation Element getAllocatedToOfAllocatedAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Allocated"), "allocatedTo");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


