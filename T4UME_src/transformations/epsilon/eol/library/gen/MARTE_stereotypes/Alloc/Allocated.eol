//setters
// -- Setter for Property Allocated::allocatedTo	
// org.eclipse.uml2.uml.internal.impl.StereotypeImpl@15178b4b (name: Allocated, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false) (isActive: false)
operation Element setAllocatedToProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_alloc = Profile.all.selectOne(e|e.name = "Alloc");
		
		var stereotype = current_alloc.getAllocatedStereotype();
		var property = current_alloc.getAllocatedToProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
