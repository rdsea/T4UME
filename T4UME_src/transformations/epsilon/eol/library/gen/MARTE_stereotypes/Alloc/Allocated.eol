//getters of Property Definitions
//--get allocatedTo Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAllocatedToPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var allocatedToProperty = allAttributes.selectOne(s|s.name = "allocatedTo");
	return allocatedToProperty;
}

//--get allocatedFrom Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAllocatedFromPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var allocatedFromProperty = allAttributes.selectOne(s|s.name = "allocatedFrom");
	return allocatedFromProperty;
}

//--get kind Property Definition. It does not change then it is cached.
@cache
operation Stereotype getKindPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var kindProperty = allAttributes.selectOne(s|s.name = "kind");
	return kindProperty;
}


/*
Check if a Set of allocatedTo of stereotype Allocated is empty.
*/ 
operation Element isAllocatedToOfAllocatedEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Allocated"), "allocatedTo");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
