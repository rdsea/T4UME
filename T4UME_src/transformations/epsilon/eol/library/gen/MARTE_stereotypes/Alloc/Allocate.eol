//getters of Property Definitions
//--get kind Property Definition. It does not change then it is cached.
@cache
operation Stereotype getKindPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var kindProperty = allAttributes.selectOne(s|s.name = "kind");
	return kindProperty;
}

//--get nature Property Definition. It does not change then it is cached.
@cache
operation Stereotype getNaturePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var natureProperty = allAttributes.selectOne(s|s.name = "nature");
	return natureProperty;
}

//--get impliedConstraint Property Definition. It does not change then it is cached.
@cache
operation Stereotype getImpliedConstraintPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var impliedConstraintProperty = allAttributes.selectOne(s|s.name = "impliedConstraint");
	return impliedConstraintProperty;
}


/*
Check if a Set of kind of stereotype Allocate is empty.
*/ 
operation Element isKindOfAllocateEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Allocate"), "kind");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
