import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with Allocate.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package AllocatesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isAllocate()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Allocate.
*/ 
operation Element isAllocate() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isAllocate() : Boolean in Allocate.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Allocate");
}


//getters
/*
Returns a Set of kind of stereotype Allocate.
*/ 
operation Element getKindOfAllocateAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Allocate"), "kind");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


