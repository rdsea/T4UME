//setters
// -- Setter for Property Allocate::kind	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@2aa0a837 (name: AllocationKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setKindProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_alloc = Profile.all.selectOne(e|e.name = "Alloc");
		
		var stereotype = current_alloc.getAllocateStereotype();
		var property = current_alloc.getKindProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
