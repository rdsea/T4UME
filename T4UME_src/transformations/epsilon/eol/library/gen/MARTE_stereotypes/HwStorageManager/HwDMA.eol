/*
Returns a Set of nbChannels of stereotype HwDMA.
*/ 
operation Element getNbChannelsOfHwDMAAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwDMA"), "nbChannels");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


