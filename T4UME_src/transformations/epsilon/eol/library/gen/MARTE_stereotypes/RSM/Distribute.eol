//getters of Property Definitions
//--get patternShape Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPatternShapePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var patternShapeProperty = allAttributes.selectOne(s|s.name = "patternShape");
	return patternShapeProperty;
}

