/*
Returns a Set of Element annotated with Distribute.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package DistributesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isDistribute()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Distribute.
*/ 
operation Element isDistribute() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isDistribute() : Boolean in Distribute.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Distribute");
}


//getters
/*
Returns a Set of patternShape of stereotype Distribute.
*/ 
operation Element getPatternShapeOfDistributeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Distribute"), "patternShape");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


