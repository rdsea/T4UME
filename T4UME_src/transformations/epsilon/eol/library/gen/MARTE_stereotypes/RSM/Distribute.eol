//setters
// -- Setter for Property Distribute::patternShape	
// org.eclipse.uml2.uml.internal.impl.DataTypeImpl@45287faf (name: ShapeSpecification, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setPatternShapeProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_rSM = Profile.all.selectOne(e|e.name = "RSM");
		
		var stereotype = current_rSM.getDistributeStereotype();
		var property = current_rSM.getPatternShapeProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
