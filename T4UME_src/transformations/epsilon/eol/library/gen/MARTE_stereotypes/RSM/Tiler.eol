/*
Returns a Set of Element annotated with Tiler.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package TilersAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isTiler()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Tiler.
*/ 
operation Element isTiler() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isTiler() : Boolean in Tiler.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Tiler");
}


//getters
/*
Returns a Set of origin of stereotype Tiler.
*/ 
operation Element getOriginOfTilerAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Tiler"), "origin");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


