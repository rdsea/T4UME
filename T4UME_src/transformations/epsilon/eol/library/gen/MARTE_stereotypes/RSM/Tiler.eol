//getters of Property Definitions
//--get origin Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOriginPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var originProperty = allAttributes.selectOne(s|s.name = "origin");
	return originProperty;
}

//--get paving Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPavingPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var pavingProperty = allAttributes.selectOne(s|s.name = "paving");
	return pavingProperty;
}

//--get fitting Property Definition. It does not change then it is cached.
@cache
operation Stereotype getFittingPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var fittingProperty = allAttributes.selectOne(s|s.name = "fitting");
	return fittingProperty;
}

//--get tiler Property Definition. It does not change then it is cached.
@cache
operation Stereotype getTilerPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var tilerProperty = allAttributes.selectOne(s|s.name = "tiler");
	return tilerProperty;
}


/*
Check if a Set of origin of stereotype Tiler is empty.
*/ 
operation Element isOriginOfTilerEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Tiler"), "origin");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
