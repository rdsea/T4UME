/*
Check if a Set of durationElements of stereotype SwTimerResource is empty.
*/ 
operation Element isDurationElementsOfSwTimerResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SwTimerResource"), "durationElements");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
