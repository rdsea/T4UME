/*
Returns a Set of Element annotated with SwTimerResource.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SwTimerResourcesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSwTimerResource()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SwTimerResource.
*/ 
operation Element isSwTimerResource() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSwTimerResource() : Boolean in SwTimerResource.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SwTimerResource");
}


//getters
/*
Returns a Set of durationElements of stereotype SwTimerResource.
*/ 
operation Element getDurationElementsOfSwTimerResourceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SwTimerResource"), "durationElements");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


