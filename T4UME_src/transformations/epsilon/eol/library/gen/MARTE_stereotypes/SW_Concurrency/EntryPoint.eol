//getters of Property Definitions
//--get isReentrant Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsReentrantPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isReentrantProperty = allAttributes.selectOne(s|s.name = "isReentrant");
	return isReentrantProperty;
}

//--get routine Property Definition. It does not change then it is cached.
@cache
operation Stereotype getRoutinePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var routineProperty = allAttributes.selectOne(s|s.name = "routine");
	return routineProperty;
}

