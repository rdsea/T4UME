/*
Returns a Set of Element annotated with EntryPoint.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package EntryPointsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isEntryPoint()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with EntryPoint.
*/ 
operation Element isEntryPoint() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isEntryPoint() : Boolean in EntryPoint.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "EntryPoint");
}


//getters
/*
Returns a Set of isReentrant of stereotype EntryPoint.
*/ 
operation Element getIsReentrantOfEntryPointAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("EntryPoint"), "isReentrant");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


