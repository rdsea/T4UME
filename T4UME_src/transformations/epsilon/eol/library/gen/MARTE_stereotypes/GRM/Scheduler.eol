/*
Returns a Set of Element annotated with Scheduler.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SchedulersAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isScheduler()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Scheduler.
*/ 
operation Element isScheduler() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isScheduler() : Boolean in Scheduler.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Scheduler");
}


//getters
/*
Returns a Set of isPreemptible of stereotype Scheduler.
*/ 
operation Element getIsPreemptibleOfSchedulerAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Scheduler"), "isPreemptible");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


