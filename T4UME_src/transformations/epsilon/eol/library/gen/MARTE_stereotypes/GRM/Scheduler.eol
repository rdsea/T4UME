/*
Check if a Set of isPreemptible of stereotype Scheduler is empty.
*/ 
operation Element isIsPreemptibleOfSchedulerEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Scheduler"), "isPreemptible");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
