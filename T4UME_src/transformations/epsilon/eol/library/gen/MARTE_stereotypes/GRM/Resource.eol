import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with Resource.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package ResourcesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isResource()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Resource.
*/ 
operation Element isResource() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isResource() : Boolean in Resource.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Resource");
}


//getters
/*
Returns a Set of resMult of stereotype Resource.
*/ 
operation Element getResMultOfResourceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Resource"), "resMult");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


