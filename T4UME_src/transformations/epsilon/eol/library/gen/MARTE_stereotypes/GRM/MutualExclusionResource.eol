//setters
// -- Setter for Property MutualExclusionResource::protectKind	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@2d7ebca4 (name: ProtectProtocolKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setProtectKindProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_gRM = Profile.all.selectOne(e|e.name = "GRM");
		
		var stereotype = current_gRM.getMutualExclusionResourceStereotype();
		var property = current_gRM.getProtectKindProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
