//--get protectKind Property Definition. It does not change then it is cached.
@cache
operation Stereotype getProtectKindPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var protectKindProperty = allAttributes.selectOne(s|s.name = "protectKind");
	return protectKindProperty;
}

