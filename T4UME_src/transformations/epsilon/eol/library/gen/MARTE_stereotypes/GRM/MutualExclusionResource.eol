/*
Check if a Set of protectKind of stereotype MutualExclusionResource is empty.
*/ 
operation Element isProtectKindOfMutualExclusionResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("MutualExclusionResource"), "protectKind");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
