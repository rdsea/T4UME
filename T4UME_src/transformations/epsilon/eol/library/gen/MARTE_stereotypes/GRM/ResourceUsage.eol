/*
Returns a Set of Element annotated with ResourceUsage.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package ResourceUsagesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isResourceUsage()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with ResourceUsage.
*/ 
operation Element isResourceUsage() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isResourceUsage() : Boolean in ResourceUsage.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "ResourceUsage");
}


//getters
/*
Returns a Set of execTime of stereotype ResourceUsage.
*/ 
operation Element getExecTimeOfResourceUsageAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("ResourceUsage"), "execTime");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


