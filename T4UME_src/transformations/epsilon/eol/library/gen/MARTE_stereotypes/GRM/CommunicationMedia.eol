/*
Check if a Set of elementSize of stereotype CommunicationMedia is empty.
*/ 
operation Element isElementSizeOfCommunicationMediaEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("CommunicationMedia"), "elementSize");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
