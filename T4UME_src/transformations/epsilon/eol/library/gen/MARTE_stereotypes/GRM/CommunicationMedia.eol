/*
Returns a Set of Element annotated with CommunicationMedia.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package CommunicationMediasAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isCommunicationMedia()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with CommunicationMedia.
*/ 
operation Element isCommunicationMedia() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isCommunicationMedia() : Boolean in CommunicationMedia.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "CommunicationMedia");
}


