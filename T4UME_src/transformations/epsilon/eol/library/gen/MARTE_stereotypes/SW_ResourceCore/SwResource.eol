/*
Check if a Set of identifierElements of stereotype SwResource is empty.
*/ 
operation Element isIdentifierElementsOfSwResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SwResource"), "identifierElements");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
