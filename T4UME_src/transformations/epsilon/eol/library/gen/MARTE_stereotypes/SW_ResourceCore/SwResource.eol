/*
Returns a Set of Element annotated with SwResource.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SwResourcesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSwResource()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SwResource.
*/ 
operation Element isSwResource() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSwResource() : Boolean in SwResource.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SwResource");
}


//getters
/*
Returns a Set of identifierElements of stereotype SwResource.
*/ 
operation Element getIdentifierElementsOfSwResourceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SwResource"), "identifierElements");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


