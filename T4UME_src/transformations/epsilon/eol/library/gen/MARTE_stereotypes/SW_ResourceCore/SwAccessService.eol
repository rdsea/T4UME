//getters of Property Definitions
//--get isModifier Property Definition. It does not change then it is cached.
@cache
operation Stereotype getIsModifierPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var isModifierProperty = allAttributes.selectOne(s|s.name = "isModifier");
	return isModifierProperty;
}

//--get accessedElement Property Definition. It does not change then it is cached.
@cache
operation Stereotype getAccessedElementPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var accessedElementProperty = allAttributes.selectOne(s|s.name = "accessedElement");
	return accessedElementProperty;
}

//--get owner Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOwnerPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var ownerProperty = allAttributes.selectOne(s|s.name = "owner");
	return ownerProperty;
}


/*
Check if a Set of isModifier of stereotype SwAccessService is empty.
*/ 
operation Element isIsModifierOfSwAccessServiceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("SwAccessService"), "isModifier");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
