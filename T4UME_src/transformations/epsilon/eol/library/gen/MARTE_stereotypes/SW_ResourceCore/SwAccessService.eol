import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with SwAccessService.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package SwAccessServicesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isSwAccessService()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with SwAccessService.
*/ 
operation Element isSwAccessService() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isSwAccessService() : Boolean in SwAccessService.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "SwAccessService");
}


//getters
/*
Returns a Set of isModifier of stereotype SwAccessService.
*/ 
operation Element getIsModifierOfSwAccessServiceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("SwAccessService"), "isModifier");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


