//setters
// -- Setter for Property SwAccessService::isModifier	
// org.eclipse.uml2.uml.internal.impl.PrimitiveTypeImpl@adbe421 (name: Boolean, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setIsModifierProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_sW_ResourceCore = Profile.all.selectOne(e|e.name = "SW_ResourceCore");
		
		var stereotype = current_sW_ResourceCore.getSwAccessServiceStereotype();
		var property = current_sW_ResourceCore.getIsModifierProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
