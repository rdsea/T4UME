/*
Check if a Set of poolSize of stereotype PaRunTInstance is empty.
*/ 
operation Element isPoolSizeOfPaRunTInstanceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("PaRunTInstance"), "poolSize");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
