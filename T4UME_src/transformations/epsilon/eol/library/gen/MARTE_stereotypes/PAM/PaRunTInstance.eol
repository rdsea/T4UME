//--get poolSize Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPoolSizePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var poolSizeProperty = allAttributes.selectOne(s|s.name = "poolSize");
	return poolSizeProperty;
}

