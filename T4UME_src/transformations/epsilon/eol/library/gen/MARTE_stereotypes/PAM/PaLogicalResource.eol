/*
Check if a Set of utilization of stereotype PaLogicalResource is empty.
*/ 
operation Element isUtilizationOfPaLogicalResourceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("PaLogicalResource"), "utilization");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
