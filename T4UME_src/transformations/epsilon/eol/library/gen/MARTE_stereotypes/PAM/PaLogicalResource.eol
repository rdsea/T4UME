/*
Returns a Set of Element annotated with PaLogicalResource.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package PaLogicalResourcesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isPaLogicalResource()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with PaLogicalResource.
*/ 
operation Element isPaLogicalResource() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isPaLogicalResource() : Boolean in PaLogicalResource.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "PaLogicalResource");
}


//getters
/*
Returns a Set of utilization of stereotype PaLogicalResource.
*/ 
operation Element getUtilizationOfPaLogicalResourceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("PaLogicalResource"), "utilization");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


