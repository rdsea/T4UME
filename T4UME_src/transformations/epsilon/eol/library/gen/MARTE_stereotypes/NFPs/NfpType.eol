/*
Returns a Set of Element annotated with NfpType.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package NfpTypesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isNfpType()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with NfpType.
*/ 
operation Element isNfpType() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isNfpType() : Boolean in NfpType.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "NfpType");
}


//getters
/*
Returns a Set of valueAttrib of stereotype NfpType.
*/ 
operation Element getValueAttribOfNfpTypeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("NfpType"), "valueAttrib");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


