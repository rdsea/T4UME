/*
Check if a Set of valueAttrib of stereotype NfpType is empty.
*/ 
operation Element isValueAttribOfNfpTypeEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("NfpType"), "valueAttrib");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
