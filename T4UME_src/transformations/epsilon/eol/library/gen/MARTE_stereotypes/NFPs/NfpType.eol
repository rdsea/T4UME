//getters of Property Definitions
//--get valueAttrib Property Definition. It does not change then it is cached.
@cache
operation Stereotype getValueAttribPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var valueAttribProperty = allAttributes.selectOne(s|s.name = "valueAttrib");
	return valueAttribProperty;
}

