//setters
// -- Setter for Property Dimension::symbol	
// org.eclipse.uml2.uml.internal.impl.PrimitiveTypeImpl@7409e50c (name: String, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setSymbolProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_nFPs = Profile.all.selectOne(e|e.name = "NFPs");
		
		var stereotype = current_nFPs.getDimensionStereotype();
		var property = current_nFPs.getSymbolProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
