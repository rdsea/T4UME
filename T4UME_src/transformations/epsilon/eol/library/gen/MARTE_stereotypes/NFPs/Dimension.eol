//getters of Property Definitions
//--get symbol Property Definition. It does not change then it is cached.
@cache
operation Stereotype getSymbolPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var symbolProperty = allAttributes.selectOne(s|s.name = "symbol");
	return symbolProperty;
}

//--get baseDimension Property Definition. It does not change then it is cached.
@cache
operation Stereotype getBaseDimensionPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var baseDimensionProperty = allAttributes.selectOne(s|s.name = "baseDimension");
	return baseDimensionProperty;
}

//--get baseExponent Property Definition. It does not change then it is cached.
@cache
operation Stereotype getBaseExponentPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var baseExponentProperty = allAttributes.selectOne(s|s.name = "baseExponent");
	return baseExponentProperty;
}


/*
Check if a Set of symbol of stereotype Dimension is empty.
*/ 
operation Element isSymbolOfDimensionEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("Dimension"), "symbol");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
