import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with Dimension.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package DimensionsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isDimension()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with Dimension.
*/ 
operation Element isDimension() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isDimension() : Boolean in Dimension.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "Dimension");
}


//getters
/*
Returns a Set of symbol of stereotype Dimension.
*/ 
operation Element getSymbolOfDimensionAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("Dimension"), "symbol");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


