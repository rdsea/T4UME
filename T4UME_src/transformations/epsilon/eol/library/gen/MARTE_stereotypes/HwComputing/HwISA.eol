//setters
// -- Setter for Property HwISA::family	
// org.eclipse.uml2.uml.internal.impl.DataTypeImpl@393616c1 (name: NFP_String, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setFamilyProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hwComputing = Profile.all.selectOne(e|e.name = "HwComputing");
		
		var stereotype = current_hwComputing.getHwISAStereotype();
		var property = current_hwComputing.getFamilyProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
