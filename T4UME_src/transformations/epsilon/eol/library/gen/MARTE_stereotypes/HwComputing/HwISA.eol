/*
Returns a Set of Element annotated with HwISA.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwISAsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwISA()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwISA.
*/ 
operation Element isHwISA() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwISA() : Boolean in HwISA.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwISA");
}


//getters
/*
Returns a Set of family of stereotype HwISA.
*/ 
operation Element getFamilyOfHwISAAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwISA"), "family");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


