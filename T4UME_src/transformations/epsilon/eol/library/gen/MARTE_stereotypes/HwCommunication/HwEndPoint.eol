/*
Returns a Set of connectedTo of stereotype HwEndPoint.
*/ 
operation Element getConnectedToOfHwEndPointAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwEndPoint"), "connectedTo");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


