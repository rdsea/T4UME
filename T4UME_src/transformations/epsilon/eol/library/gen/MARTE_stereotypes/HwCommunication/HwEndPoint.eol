//setters
// -- Setter for Property HwEndPoint::connectedTo	
// org.eclipse.uml2.uml.internal.impl.StereotypeImpl@47faf39e (name: HwMedia, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false) (isActive: false)
operation Element setConnectedToProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_hwCommunication = Profile.all.selectOne(e|e.name = "HwCommunication");
		
		var stereotype = current_hwCommunication.getHwEndPointStereotype();
		var property = current_hwCommunication.getConnectedToProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
