/*
Check if a Set of connectedTo of stereotype HwEndPoint is empty.
*/ 
operation Element isConnectedToOfHwEndPointEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("HwEndPoint"), "connectedTo");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
