/*
Returns a Set of Element annotated with HwArbiter.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwArbitersAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwArbiter()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwArbiter.
*/ 
operation Element isHwArbiter() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwArbiter() : Boolean in HwArbiter.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwArbiter");
}


//getters
/*
Returns a Set of controlledMedias of stereotype HwArbiter.
*/ 
operation Element getControlledMediasOfHwArbiterAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwArbiter"), "controlledMedias");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


