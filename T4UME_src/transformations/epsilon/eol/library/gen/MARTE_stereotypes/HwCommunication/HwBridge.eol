/*
Returns a Set of Element annotated with HwBridge.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwBridgesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwBridge()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwBridge.
*/ 
operation Element isHwBridge() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwBridge() : Boolean in HwBridge.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwBridge");
}


//getters
/*
Returns a Set of sides of stereotype HwBridge.
*/ 
operation Element getSidesOfHwBridgeAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwBridge"), "sides");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


