//getters of Property Definitions
//--get ordering Property Definition. It does not change then it is cached.
@cache
operation Stereotype getOrderingPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var orderingProperty = allAttributes.selectOne(s|s.name = "ordering");
	return orderingProperty;
}

//--get insertion Property Definition. It does not change then it is cached.
@cache
operation Stereotype getInsertionPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var insertionProperty = allAttributes.selectOne(s|s.name = "insertion");
	return insertionProperty;
}

//--get selection Property Definition. It does not change then it is cached.
@cache
operation Stereotype getSelectionPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var selectionProperty = allAttributes.selectOne(s|s.name = "selection");
	return selectionProperty;
}


/*
Check if a Set of ordering of stereotype DataPool is empty.
*/ 
operation Element isOrderingOfDataPoolEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("DataPool"), "ordering");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
