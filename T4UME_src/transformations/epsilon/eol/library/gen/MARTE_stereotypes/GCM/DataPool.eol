//setters
// -- Setter for Property DataPool::ordering	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@6db03906 (name: DataPoolOrderingKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setOrderingProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_gCM = Profile.all.selectOne(e|e.name = "GCM");
		
		var stereotype = current_gCM.getDataPoolStereotype();
		var property = current_gCM.getOrderingProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
