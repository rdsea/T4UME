import "../../../utilities.eol";

//do not import the profile library to avoid circular dependencies


/*
Returns a Set of Element annotated with DataPool.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package DataPoolsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isDataPool()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with DataPool.
*/ 
operation Element isDataPool() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isDataPool() : Boolean in DataPool.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "DataPool");
}


//getters
/*
Returns a Set of ordering of stereotype DataPool.
*/ 
operation Element getOrderingOfDataPoolAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("DataPool"), "ordering");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


