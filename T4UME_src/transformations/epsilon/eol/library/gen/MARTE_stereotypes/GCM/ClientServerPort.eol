// -- Setter for Property ClientServerPort::specificationKind	
// org.eclipse.uml2.uml.internal.impl.EnumerationImpl@43984e6b (name: PortSpecificationKind, visibility: <unset>) (isLeaf: false, isAbstract: false, isFinalSpecialization: false)
operation Element setSpecificationKindProperty( value : Object) {
		//setStereotypePropertyValue(NamedElement namedElement, Stereotype stereotype, Property property, Object value) {
		
		var plugin : new Native("at.ac.tuwien.dsg.T4UME");
		var UMLUtil = plugin.getUMLUtil();
		var ModelerAgent = plugin.getModelerAgent();	
		
		var current_gCM = Profile.all.selectOne(e|e.name = "GCM");
		
		var stereotype = current_gCM.getClientServerPortStereotype();
		var property = current_gCM.getSpecificationKindProperty();
		ModelerAgent.setStereotypePropertyValue(self, stereotype, property, value);
}
