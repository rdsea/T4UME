/*
Returns a Set of Element annotated with ClientServerPort.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package ClientServerPortsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isClientServerPort()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with ClientServerPort.
*/ 
operation Element isClientServerPort() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isClientServerPort() : Boolean in ClientServerPort.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "ClientServerPort");
}


