/*
Returns a Set of Element annotated with GaWorkloadEvent.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package GaWorkloadEventsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isGaWorkloadEvent()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with GaWorkloadEvent.
*/ 
operation Element isGaWorkloadEvent() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isGaWorkloadEvent() : Boolean in GaWorkloadEvent.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "GaWorkloadEvent");
}


//getters
/*
Returns a Set of pattern of stereotype GaWorkloadEvent.
*/ 
operation Element getPatternOfGaWorkloadEventAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("GaWorkloadEvent"), "pattern");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


