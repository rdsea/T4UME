/*
Check if a Set of latency of stereotype GaLatencyObs is empty.
*/ 
operation Element isLatencyOfGaLatencyObsEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("GaLatencyObs"), "latency");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
