/*
Returns a Set of Element annotated with GaLatencyObs.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package GaLatencyObssAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isGaLatencyObs()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with GaLatencyObs.
*/ 
operation Element isGaLatencyObs() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isGaLatencyObs() : Boolean in GaLatencyObs.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "GaLatencyObs");
}


//getters
/*
Returns a Set of latency of stereotype GaLatencyObs.
*/ 
operation Element getLatencyOfGaLatencyObsAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("GaLatencyObs"), "latency");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


