//getters of Property Definitions
//--get context Property Definition. It does not change then it is cached.
@cache
operation Stereotype getContextPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var contextProperty = allAttributes.selectOne(s|s.name = "context");
	return contextProperty;
}

//--get workload Property Definition. It does not change then it is cached.
@cache
operation Stereotype getWorkloadPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var workloadProperty = allAttributes.selectOne(s|s.name = "workload");
	return workloadProperty;
}

//--get platform Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPlatformPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
