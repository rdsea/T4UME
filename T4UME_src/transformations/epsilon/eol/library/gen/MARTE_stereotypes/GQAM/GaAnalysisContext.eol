}


/*
Check if a Set of context of stereotype GaAnalysisContext is empty.
*/ 
operation Element isContextOfGaAnalysisContextEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("GaAnalysisContext"), "context");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
