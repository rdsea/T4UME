/*
Returns a Set of Element annotated with GaAnalysisContext.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package GaAnalysisContextsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isGaAnalysisContext()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with GaAnalysisContext.
*/ 
operation Element isGaAnalysisContext() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isGaAnalysisContext() : Boolean in GaAnalysisContext.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "GaAnalysisContext");
}


//getters
/*
Returns a Set of context of stereotype GaAnalysisContext.
*/ 
operation Element getContextOfGaAnalysisContextAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("GaAnalysisContext"), "context");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


