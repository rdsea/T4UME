//getters of Property Definitions
//--get laxity Property Definition. It does not change then it is cached.
@cache
operation Stereotype getLaxityPropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var laxityProperty = allAttributes.selectOne(s|s.name = "laxity");
	return laxityProperty;
}

