/*
Check if a Set of laxity of stereotype GaTimedObs is empty.
*/ 
operation Element isLaxityOfGaTimedObsEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("GaTimedObs"), "laxity");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
