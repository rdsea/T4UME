/*
Returns a Set of Element annotated with GaTimedObs.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package GaTimedObssAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isGaTimedObs()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with GaTimedObs.
*/ 
operation Element isGaTimedObs() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isGaTimedObs() : Boolean in GaTimedObs.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "GaTimedObs");
}


//getters
/*
Returns a Set of laxity of stereotype GaTimedObs.
*/ 
operation Element getLaxityOfGaTimedObsAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("GaTimedObs"), "laxity");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


