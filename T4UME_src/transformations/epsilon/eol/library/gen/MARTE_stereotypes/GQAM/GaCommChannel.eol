/*
Returns a Set of Element annotated with GaCommChannel.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package GaCommChannelsAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isGaCommChannel()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with GaCommChannel.
*/ 
operation Element isGaCommChannel() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isGaCommChannel() : Boolean in GaCommChannel.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "GaCommChannel");
}


//getters
/*
Returns a Set of packetSize of stereotype GaCommChannel.
*/ 
operation Element getPacketSizeOfGaCommChannelAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("GaCommChannel"), "packetSize");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


