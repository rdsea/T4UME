/*
Check if a Set of packetSize of stereotype GaCommChannel is empty.
*/ 
operation Element isPacketSizeOfGaCommChannelEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("GaCommChannel"), "packetSize");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
