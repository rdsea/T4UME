//--get packetSize Property Definition. It does not change then it is cached.
@cache
operation Stereotype getPacketSizePropertyDefinition() : Property {
	var allAttributes = self.getAllAttributes();
	var packetSizeProperty = allAttributes.selectOne(s|s.name = "packetSize");
	return packetSizeProperty;
}

