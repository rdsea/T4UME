/*
Returns a Set of Element annotated with HwDevice.
TODO: Currently the method does not distinguish between different UML Model Elements. All the methods SetOf* are equal.
*/ 

operation Package HwDevicesAsSet() : Set {
	var returnValue = new Set;
	for(son in self.getPackagedElements()){
		if(son.isHwDevice()){
			returnValue.add(son);
		}
	} 
	return returnValue;
}
 

/*
Check if the element is annotated with HwDevice.
*/ 
operation Element isHwDevice() : Boolean {
	//var plugin : new Native("at.ac.tuwien.dsg.T4UME");
	//var UMLUtil = plugin.getUMLUtil();
	//var ModelerAgent = plugin.getModelerAgent();	
	
	/*
	if(self.isUndefined() or self.getAppliedStereotypes().isUndefined() ){
		("the element is undefined").println();
		showError("Method: isHwDevice() : Boolean in HwDevice.eol");
	}
	*/
	return self.getAppliedStereotypes()->exists(s | s.name = "HwDevice");
}


//getters
/*
Returns a Set of description of stereotype HwDevice.
*/ 
operation Element getDescriptionOfHwDeviceAsSet() : Set {
	var returnValues = new Set;
	var values = self.getValue(self.getStereotype("HwDevice"), "description");
	for(value in values){
		returnValues.add(value);
	}
	return returnValues;
}


