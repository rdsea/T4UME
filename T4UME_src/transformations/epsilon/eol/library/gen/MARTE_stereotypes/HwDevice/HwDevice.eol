/*
Check if a Set of description of stereotype HwDevice is empty.
*/ 
operation Element isDescriptionOfHwDeviceEmpty() : Boolean{
	var returnValues = new Set();
	var values = self.getValue(self.getStereotype("HwDevice"), "description");
	for(value in values){
		returnValues.add(value);
	}
	var empty = returnValues.isEmpty();
	return empty;
}
